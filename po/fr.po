#: ../src/notification.c:145 ../src/notification.c:151
#: ../src/notification.c:154 ../src/input.c:91
msgid "Birthday Reminder"
msgstr "Rappel d'Anniversaires"

#: ../src/birthday_reminder.c:138
msgid "Reminds you of the birthday of your buddies. :o)"
msgstr "Vous rappelle les anniversaires de vos ami(e)s. :o)"

#: ../src/birthday_reminder.c:139
msgid "This Plugin shall keep track and remind you of your buddies' birthdays."
msgstr ""
"Ce greffon tient à jour les anniversaires de vos ami(e) et vous rappelle "
"quand une date approche."

#. Translators: to use an other orde of the arguments use %2$02d for day, %1$02d for month and %3$04d for year
#: ../src/tooltip.c:40
#, c-format
msgid ""
"\n"
"<b>Birthday</b>: %02d/%02d/%04d"
msgstr ""
"\n"
"<b>Anniversaire</b> : %2$02d/%1$02d/%3$04d"

#. Translators: use %2$02d for day and %1$02d for month
#: ../src/tooltip.c:43
#, c-format
msgid ""
"\n"
"<b>Birthday</b>: %02d/%02d"
msgstr ""
"\n"
"<b>Anniversaire</b> : %2$02d.%1$02d."

#: ../src/tooltip.c:49
#, c-format
msgid " (<b>Today!</b>)"
msgstr " (<b>Aujourd'hui !</b>)"

#: ../src/tooltip.c:51
#, c-format
msgid " (<b>Tomorrow!</b>)"
msgstr " (<b>Demain !</b>)"

#: ../src/tooltip.c:53
#, c-format
msgid " (in %d days)"
msgstr " (dans %d jours)"

#: ../src/tooltip.c:60
#, c-format
msgid ""
"\n"
"<b>Age</b>: %d years"
msgstr ""
"\n"
"<b>Age</b>: %d ans"

