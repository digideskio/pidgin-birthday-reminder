msgstr "Impossible de charger les icônes !\n"

#: ../src/notification.c:64
#, c-format
msgid "sound file (%s) does not exist.\n"
msgstr "Le fichier son (%s) est manquant.\n"

#: ../src/notification.c:116
#, c-format
msgid "%s will be %d years old today! Hooray!"
msgstr "%s va avoir %d ans aujourd'hui ! Youpi !"

#: ../src/notification.c:118
#, c-format
msgid "It's %s's birthday today! Hooray!"
msgstr "C'est l'anniversaire de %s aujourd'hui ! Youpi !"

#: ../src/notification.c:122
#, c-format
msgid "%s will be %d years old tomorrow!"
msgstr "%s aura %d ans demain !"

#: ../src/notification.c:124
#, c-format
msgid "%s's birthday is tomorrow!"
msgstr "L'anniversaire de %s aura lieu demain !"

#: ../src/notification.c:128
#, c-format
msgid "%s will be %d years old in %d days!"
msgstr "%s aura %d ans dans %d jours !"

#: ../src/notification.c:130
#, c-format
msgid "%s's birthday is in %d days!"
msgstr "L'anniversaire de %s aura lieu dans %d jours !"

#: ../src/notification.c:135
msgid "There are birthdays in the next days! Hooray!"
msgstr "Il va y avoir des anniversaires dans les prochains jours ! Youpi !"

